-e
--exclude=.cask
--exclude=flycheck_*
--exclude=.git
--exclude=.stack-work
--langdef=scala
--langmap=scala:.scala
--regex-scala=/\bclass\s+(\w+)/\1/c,classes/
--regex-scala=/\bobject\s+(\w+)/\1/c,objects/
--regex-scala=/\btrait\s+(\w+)/\1/t,traits/
--regex-scala=/\btype\s+(\w+)/\1/T,types/
--regex-scala=/\bdef\s+(\w+)/\1/m,methods/
--regex-scala=/\bpackage\s+((\w|[.])+)/\1/p,packages/
--langdef=haskell
--langmap=haskell:.hs
--regex-haskell=/\bdata\s+(\S+)/\1/d,data/
--regex-haskell=/\btype\s+(\S+)/\1/t,types/
--regex-haskell=/\bclass\s+(\([^=]+\)\s+=>\s+)?(\S+)/\2/c,class/
--regex-haskell=/\binstance\s+(\([^=]+\)\s+=>\s+)?(\S+)/\2/i,instance/
--regex-haskell=/\bmodule\s+((\S|[.])+)/\1/m,module/
--regex-haskell=/\b(\S+)\s+::/\1/f,function/
--languages=c,lisp,haskell

